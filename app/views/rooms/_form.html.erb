<%= form_with(model: room,
              class:'d-flex',
              data: {
                controller: "reset_form",
                action: "turbo:submit-end->reset_form#reset" } ) do |form| %>
  <%= form.text_field :name, class: 'form-control', autocomplete: 'off' %>
  <%= form.submit data: {disable_with: false} %>
<% end %>
